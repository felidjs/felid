{
  "name": "felid",
  "version": "0.0.4",
  "description": "A Node.js web framework",
  "main": "src/index.js",
  "scripts": {
    "benchmark": "concurrently -k -s first \"node ./examples/basic.js\" \"autocannon -c 100 -d 5 -p 10 localhost:8080\"",
    "coverage": "codecov",
    "lint": "standard --fix src/**/*.js test/**/*.js | snazzy",
    "test": "npm run lint && npm run unit",
    "unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felidjs/felid.git"
  },
  "keywords": [
    "web",
    "framework",
    "api"
  ],
  "author": "zoron (https://github.com/fralonra/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felidjs/felid/issues"
  },
  "homepage": "https://github.com/felidjs/felid#readme",
  "devDependencies": {
    "autocannon": "^4.0.0",
    "codecov": "^3.6.1",
    "concurrently": "^4.1.2",
    "injectar": "^1.4.0",
    "jest": "^24.9.0",
    "pre-commit": "^1.2.2",
    "snazzy": "^8.0.0",
    "standard": "^14.0.0",
    "superagent": "^5.1.0"
  },
  "dependencies": {
    "abstract-logging": "^2.0.0",
    "delegates": "^1.0.0",
    "find-my-way": "^2.1.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
